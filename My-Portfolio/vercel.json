{
    "builds": [
        {
            "src": "src/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/spa/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}